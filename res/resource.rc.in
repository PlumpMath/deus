#include <windows.h>
#include <resource.h>
#pragma code_page(65001)

// Icons
IDI_MAIN ICON "main.ico"

// Data
@DATA_RESOURCES@
// Info
VS_VERSION_INFO VERSIONINFO
  PRODUCTVERSION  @PROJECT_VERSION_MAJOR@, @PROJECT_VERSION_MINOR@
  FILEVERSION     @PROJECT_VERSION_MAJOR@, @PROJECT_VERSION_MINOR@, @PROJECT_VERSION_PATCH@, 0
  FILEOS          VOS_NT_WINDOWS32
  FILETYPE        VFT_APP
  FILESUBTYPE     VFT2_UNKNOWN
  FILEFLAGSMASK   VS_FFI_FILEFLAGSMASK
#ifdef _DEBUG
  FILEFLAGS       VS_FF_DEBUG
#else
  FILEFLAGS       0
#endif
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "000004B0"
    BEGIN
      VALUE "CompanyName",      "@COMPANY_TEXT@"
      VALUE "ProductName",      "@COMPANY_TEXT@ @PROJECT_TEXT@"
      VALUE "ProductVersion",   "@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@"
      VALUE "OriginalFilename", "@PROJECT_NAME@.exe"
      VALUE "FileDescription",  "@PROJECT_TEXT@"
      VALUE "LegalCopyright",   "@COPYING_TEXT@"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0, 0x04B0
  END
END